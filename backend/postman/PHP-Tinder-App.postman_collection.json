    {
      "name": "List People (paginated)",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Status code is 200', function() {",
              "  pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test('Response is JSON', function() {",
              "  pm.response.to.be.json;",
              "});",
              "",
              "pm.test('Has data array', function() {",
              "  var data = pm.response.json();",
              "  pm.expect(data).to.have.property('data');",
              "  pm.expect(data.data).to.be.an('array');",
              "});",
              "",
              "pm.test('Person has required fields', function() {",
              "  var data = pm.response.json();",
              "  if (data.data.length > 0) {",
              "    var person = data.data[0];",
              "    pm.expect(person).to.have.property('id');",
              "    pm.expect(person).to.have.property('name');",
              "    pm.expect(person).to.have.property('age');",
              "    pm.expect(person).to.have.property('pictures');",
              "  }",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Accept",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "disabled": true
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/people?page=1",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "api",
            "people"
          ],
          "query": [
            {
              "key": "page",
              "value": "1"
            }
          ]
        }
